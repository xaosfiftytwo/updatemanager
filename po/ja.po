#: ../usr/lib/solydxk/updatemanager/umrefresh.py:53
#, python-format
msgid "New update: %s"
msgstr "新たなアップデート: %s"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:57
#: ../usr/lib/solydxk/updatemanager/umglobal.py:49
msgid "There are updates available"
msgstr "利用できるアップデートが存在します"

#: ../usr/lib/solydxk/updatemanager/umrefresh.py:71
msgid "Unable to retrieve sources information"
msgstr "ソース情報を見つける事ができません"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:79
msgid "Apt error"
msgstr "APT エラー"

#: ../usr/lib/solydxk/updatemanager/updatemanager.py:80
msgid "The following packages will be upgraded:"
msgstr "これらのパッケージも更新されます:"

